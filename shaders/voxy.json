{
    "unusedString":
        "
            // voxy.json is run through the Iris preprocessor, so we can include
            // Settings.glsl and use shader settings in this file.
            #include "/Settings.glsl"
        ",
    "version": 1,
    "excludeLodsFromVanillaDepth": true,
    "opaqueDrawBuffers": [
        6,
        7,
        3
    ],
    "translucentDrawBuffers": [
        7,
        2,
        3
    ],
    "blending": {
        -1: "off"
    },
#ifdef TAA_ENABLED
    "taaOffset": "{ return taaOffset; }",
    "useViewportDims": true,
#endif
    "uniforms": [
        "taaOffset",
        "gbufferModelView"
    ],
    "samplers": []
}
